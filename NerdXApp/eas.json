{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EAS_NO_VCS": "1",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "node": "20.19.4"
    },

    "preview": {
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EAS_NO_VCS": "1",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "node": "20.19.4"
    },

    "production": {
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EAS_NO_VCS": "1",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_MAVEN_CACHE_ENABLED": "false"
      },
      "node": "20.19.4",
      "cache": {
        "disabled": true
      }
    }
  }
}
