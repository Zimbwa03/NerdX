{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "node": "20.19.3"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "node": "20.19.3"
    }
  }
}