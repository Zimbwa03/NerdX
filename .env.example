# Environment Variables for NerdX
# Copy this file to .env and fill in your values

# DeepSeek AI API Key (for Mathematics & English question generation)
DEEPSEEK_API_KEY=sk-5e3b99e25a5246eb8df7f480e4989677
# DeepSeek model aliases (latest by default)
DEEPSEEK_CHAT_MODEL=deepseek-chat
DEEPSEEK_REASONER_MODEL=deepseek-reasoner

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (required for admin dashboard and bot)
# Dashboard needs: SUPABASE_URL + SUPABASE_SERVICE_ROLE_KEY (anon optional)
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# JWT Secret for Mobile API
JWT_SECRET=your_jwt_secret_here

# School credits: daily reset at 06:00 in this timezone (e.g. Africa/Harare, UTC)
SCHOOL_CREDITS_RESET_TIMEZONE=Africa/Harare

# Other API Keys
DESMOS_API_KEY=your_desmos_api_key_here

# Twilio Configuration (for WhatsApp messaging)
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here
